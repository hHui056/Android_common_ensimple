<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allen.androidcommonexample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/m_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AlertDialog.AppCompat">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".rxbus.SecondActivity"
            android:label="SecondActivity" />
        <activity
            android:name=".animator.AnimatorActivity"
            android:label="AnimatorActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".socket.SocketActivity"
            android:label="SocketActivity" />
        <activity android:name=".test.ListWithSpinnerActivity" />
        <activity
            android:name=".opengl.OpenGLActivity"
            android:label="OpenGL ES" />
    </application>

</manifest>