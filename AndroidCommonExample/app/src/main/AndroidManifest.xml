<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allen.androidcommonexample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AlertDialog.AppCompat">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SecondActivity"
            android:label="SecondActivity" />
        <activity
            android:name=".activity.AnimatorActivity"
            android:label="AnimatorActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.SocketActivity"
            android:label="SocketActivity" />
        <activity android:name=".activity.ListWithSpinnerActivity" />
        <activity android:name=".opengl.OpenGLActivity"></activity>
    </application>

</manifest>